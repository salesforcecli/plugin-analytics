{
  "name": "@salesforce/analytics",
  "description": "Commands for working with Salesforce Analytics applications, assets, and services",
  "version": "1.3.18",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/analyticsdx-vscode/issues",
  "dependencies": {
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3.3.1",
    "@salesforce/command": "^4.2.2",
    "@salesforce/core": "^2.36.0",
    "moment": "^2.24.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-command-snapshot": "^2.2.2",
    "@salesforce/cli-plugins-testkit": "^1.1.5",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^2.0.3",
    "@salesforce/plugin-command-reference": "^1.6.18",
    "@salesforce/prettier-config": "^0.0.3",
    "@salesforce/ts-sinon": "1.4.19",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "^4.3.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.2.0",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.0",
    "shx": "0.3.4",
    "sinon": "^11.1.2",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-analytics",
  "keywords": [
    "analytics",
    "analyticsdx",
    "crma",
    "crm analytics",
    "tableau",
    "tableau crm",
    "tcrm",
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "analytics": {
        "description": "commands for Salesforce Analytics developers",
        "subtopics": {
          "app": {
            "description": "create and manage analytics applications"
          },
          "asset": {
            "description": "manage analytics asset publishing",
            "subtopics": {
              "publisher": {
                "description": "manage the development user experience for publishing dashboards"
              }
            }
          },
          "autoinstall": {
            "description": "manage analytics auto-install requests",
            "subtopics": {
              "app": {
                "description": "manage analytics application auto-install requests"
              }
            }
          },
          "dashboard": {
            "description": "manage analytics dashboards",
            "subtopics": {
              "history": {
                "description": "view and manage dashboard history"
              }
            }
          },
          "dataflow": {
            "description": "manage analytics dataflows",
            "subtopics": {
              "history": {
                "description": "view and manage dataflow history"
              },
              "job": {
                "description": "view and manage dataflow jobs"
              }
            }
          },
          "dataset": {
            "description": "manage analytics datasets",
            "subtopics": {
              "rows": {
                "description": "view dataset rows"
              }
            }
          },
          "lens": {
            "description": "manage analytics lenses",
            "subtopics": {
              "history": {
                "description": "view and manage lens history"
              }
            }
          },
          "recipe": {
            "description": "manage analytics recipess"
          },
          "template": {
            "description": "create and manage analytics templates"
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-analytics",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:extras": "shx rm -rf lib && shx rm -f .prettierrc.json",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postclean": "yarn run clean:extras",
    "postclean-all": "yarn run clean:extras",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install && shx rm -f .prettierrc.json",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/run commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/run snapshot:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif-dev readme"
  },
  "publishConfig": {
    "access": "public"
  }
}